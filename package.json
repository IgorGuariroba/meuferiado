{
  "name": "cidades-vizinhas",
  "version": "1.0.0",
  "description": "API para encontrar cidade atual e cidades vizinhas usando Google Maps API",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nest start --watch",
    "start:api": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "down": "./scripts/down.sh",
    "down:volumes": "./scripts/down.sh -v",
    "service:down": "ts-node src/cli.ts service:down"
  },
  "keywords": [
    "google-maps",
    "places-api",
    "geocoding",
    "cidades"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.40",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^16.3.1",
    "mongoose": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/testing": "^11.1.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "nest-commander": "^3.20.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
